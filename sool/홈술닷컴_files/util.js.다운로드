function leadingZeros(a,b){var c="";a=a.toString();if(a.length<b)for(i=0;i<b-a.length;i++)c+="0";return c+a}function checkIE11_rv(){var a=navigator.userAgent.toLowerCase(),b=checkUsrBrw();return"ie11"==b?-1==a.indexOf("msie")&&-1<a.indexOf("trident")&&-1<a.indexOf("rv:11")?"rv11":b:b}
function checkUsrBrw(){var a="",b=navigator.userAgent.toLowerCase();-1<b.indexOf("chrome")&&-1>=b.indexOf("edge")?a="chrome":-1<b.indexOf("firefox")&&-1>=b.indexOf("edge")?a="firefox":-1<b.indexOf("safari")&&-1>=b.indexOf("edge")?a="safari":-1<b.indexOf("msie")?(a="ie",b=detectIE(),a+=b):-1<b.indexOf("trident")?a="ie11":-1<b.indexOf("edge")&&(a="ieEdge");return KCP_ORDER_BRW=a}
function detectIE(){var a=-1,b=navigator.userAgent.toLowerCase(),c=navigator.appName;if(-1<b.indexOf("msie 7")&&-1<b.indexOf("trident"))try{var d=document.body.style,e=document.createElement("canvas");return e.getContext("webgl")?"msTransition"in d||"transition"in d?"getContext"in e?11:8:9:10}catch(f){return"getContext"in e?7:8}else return"Microsoft Internet Explorer"==c?null!=/msie ([0-9]{1,}[.0-9]{0,})/.exec(b)&&(a=parseFloat(RegExp.$1)):"Netscape"==c&&(a=11),a}
function checkSirooUse(a){try{if(!("undefined"!=typeof a.enc_type&&"MD"==a.enc_type.value||"undefined"==typeof a.card_code||"CCKM"!=a.card_code.value&&"CCKB"!=a.card_code.value&&"CCKA"!=a.card_code.value)){if("undefined"!=typeof a.payco_direct&&"Y"==a.payco_direct.value||"undefined"!=typeof a.sspay_direct&&"Y"==a.sspay_direct.value||"undefined"!=typeof a.lpay_direct&&"Y"==a.lpay_direct.value||"undefined"!=typeof a.kakaopay_direct&&"Y"==a.kakaopay_direct.value)return!1;if("undefined"!=typeof a.app_flag){if("Y"==
a.app_flag.value)return checkNotKBPay(a.issue_code);if("N"==a.app_flag.value)return!1}return"undefined"!=typeof a.kcp_kbRedirect&&"Y"==a.kcp_kbRedirect.value?!1:"CCKM"==a.card_code.value?checkNotKBPay(a.issue_code):"CCKA"==a.card_code.value?!1:!0}return!1}catch(b){alert("필수 인증정보(card_code)가 올바르지 않습니다. 재시도 바랍니다.")}}function checkNotKBPay(a){return"undefined"==typeof a||"KA"!=a.value&&"HS"!=a.value&&"KNI"!=a.value&&"HWH"!=a.value&&"PYC"!=a.value?!0:!1};
